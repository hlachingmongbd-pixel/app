{
    "version": 2,
    "buildCommand": "npx expo export --platform web",
    "outputDirectory": "dist",
    "cleanUrls": true,
    "functions": {
        "api/**/*.ts": {
            "maxDuration": 10
        }
    },
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        },
        {
            "source": "/((?!api/).*)",
            "destination": "/index.html"
        }
    ]
}